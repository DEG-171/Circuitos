library ieee;
use ieee.std_logic_1164.all;

entity mux4x1_REG_IN_0 is
generic(N : natural:= 32);
port( MAP1_0,MAP2_0,MAP3_0,MAP4_0: in std_logic_vector((N-1) downto 0);
		SW: in std_logic_vector(8 downto 7);
		REG_IN_0: out std_logic_vector((N-1) downto 0)
		);
end mux4x1_REG_IN_0;

architecture mux of mux4x1_REG_IN_0 is
begin
REG_IN_0 <= MAP1_0 when SW = "00" else
	MAP2_0 when SW = "01" else
	MAP3_0 when SW = "10" else
	MAP4_0;
end mux;
